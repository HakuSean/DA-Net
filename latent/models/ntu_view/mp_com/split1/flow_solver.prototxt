net: "models/ntu_view/mp_com/split1/flow_train_val.prototxt"

# testing parameter
test_iter: 2000 # 195
test_interval: 1000
test_initialization: true

# output
display: 20
average_loss: 20
snapshot: 1000
snapshot_prefix: "models/ntu_view/mp_com/caffemodel/flow_split1"
debug_info: false

# learning rate
base_lr: 0.0001 #0.001
lr_policy: "step"
gamma: 0.1
stepsize: 5000
max_iter: 20000
iter_size: 1

# parameter of SGD
momentum: 0.9
weight_decay: 0.0005
clip_gradients: 30

# GPU setting
solver_mode: GPU
device_id: [0,1]
richness: 200
